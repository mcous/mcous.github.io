{
  "private": true,
  "name": "mike-cousins-dot-io",
  "version": "2.0.0",
  "author": "Mike Cousins <mike@cousins.io> (https://mike.cousins.io)",
  "description": "Mike Cousins' home on the internet.",
  "homepage": "https://mike.cousins.io",
  "repository": "github:mcous/mcous.github.io",
  "bugs": "https://github.com/mcous/mcous.github.io/issues",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "lint": "eslint \"src/**/*.@(ts|tsx)\" \"scripts/**/*.mjs\" \"*.config.ts\"",
    "format": "prettier --write \"src/**/*.@(ts|tsx|svg)\" \"scripts/**/*.mjs\" \"*.config.ts\" \"index.html\"",
    "check": "tsc",
    "clean": "rimraf dist dist-ssr",
    "build": "npm run clean && npm run build:static && npm run build:ssr && npm run build:prerender",
    "build:static": "vite build",
    "build:ssr": "vite build --out-dir dist-ssr --ssr src/ssr.tsx",
    "build:prerender": "node ./scripts/prerender.mjs",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.5.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "preact": "^10.6.4"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.6",
    "@prettier/plugin-xml": "^1.2.0",
    "@types/node": "^17.0.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "gh-pages": "^3.2.3",
    "preact-render-to-string": "^5.1.19",
    "prettier": "^2.5.1",
    "rehype": "^12.0.0",
    "rehype-format": "^4.0.1",
    "rehype-parse": "^8.0.3",
    "rehype-rewrite": "^3.0.4",
    "rimraf": "^3.0.2",
    "to-vfile": "^7.2.2",
    "typescript": "^4.5.4",
    "unified": "^10.1.1",
    "vite": "^2.7.7",
    "vite-plugin-windicss": "^1.6.1",
    "windicss": "^3.4.1"
  },
  "eslintConfig": {
    "extends": [
      "standard-with-typescript",
      "prettier"
    ],
    "parserOptions": {
      "project": "./tsconfig.eslint.json"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
